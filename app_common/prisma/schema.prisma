generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id        Int      @id @default(autoincrement())
  detailUrl String   @unique @map("detail_url") @db.Text
  title     String
  imageUrl  String?  @map("image_url") @db.Text
  price     Int?

  @@map("products")
}